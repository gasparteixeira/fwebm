{
  "name": "fwebm",
  "version": "0.1.0-beta",
  "private": true,
  "description": "Simple client to generate webm videos from images",
  "author": "Gaspar Teixeira <gaspar.teixeira@gamil.com>",
  "build": {
    "productName": "FWEBM",
    "appId": "com.ford.fwebm",
    "copyright": "Copyright Â© 2019",
    "generateUpdatesFilesForAllChannels": true,
    "mac": {
      "category": "your.app.category.type"
    }
  },
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.2.1",
    "electron-is-dev": "^1.1.0",
    "ffmpeg-static": "^2.5.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "typeface-roboto": "0.0.75",
    "wait-on": "^3.3.0"
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.12",
    "@rescripts/rescript-env": "^0.0.10",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "electron": "^6.0.11",
    "electron-builder": "^21.1.1"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "rm -rf dist && electron-builder -mw -c.mac.identity=null"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
